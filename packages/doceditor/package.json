{
  "name": "@docspace/doceditor",
  "version": "3.0.2",
  "private": true,
  "scripts": {
    "build:translations": "node ./scripts/buildTranslations.js",
    "build": "next build",
    "start": "NODE_ENV=development node server.js",
    "start-prod": "NODE_ENV=production node server.js",
    "start:internal": "NODE_ENV=development API_HOST=http://127.0.0.1 node server.js",
    "start-prod:internal": "NODE_ENV=production API_HOST=http://127.0.0.1 node server.js",
    "lint": "next lint --max-warnings=0",
    "clean": "shx rm -rf .next",
    "deploy": "node ./scripts/deploy.js"
  },
  "dependencies": {
    "@docspace/shared": "workspace:*",
    "@onlyoffice/document-editor-react": "catalog:",
    "@serdnam/pino-cloudwatch-transport": "catalog:",
    "framer-motion": "catalog:",
    "i18next": "catalog:",
    "moment": "catalog:",
    "nconf": "catalog:",
    "next": "catalog:",
    "pino-pretty": "catalog:",
    "pino-roll": "catalog:",
    "pino": "catalog:",
    "react-device-detect": "catalog:",
    "react-dom": "catalog:",
    "react-i18next": "catalog:",
    "react": "catalog:",
    "sass": "catalog:",
    "styled-components": "catalog:",
    "ts-pattern": "catalog:"
  },
  "devDependencies": {
    "@svgr/webpack": "catalog:",
    "@types/nconf": "catalog:",
    "@types/node": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react": "catalog:",
    "@types/styled-components": "catalog:",
    "eslint-config-next": "catalog:",
    "eslint": "catalog:",
    "nconf": "catalog:",
    "pino-pretty": "catalog:",
    "pino-roll": "catalog:",
    "pino": "catalog:",
    "prettier": "catalog:",
    "shx": "catalog:",
    "terser-webpack-plugin": "catalog:",
    "typescript": "catalog:"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start": {
        "dependsOn": [
          "build"
        ]
      },
      "start-prod": {
        "dependsOn": [
          "build"
        ]
      },
      "start:internal": {
        "dependsOn": [
          "build"
        ]
      },
      "start-prod:internal": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
