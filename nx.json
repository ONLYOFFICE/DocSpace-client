{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "before-build": {
      "cache": true,
      "outputs": ["{workspaceRoot}/packages/runtime.json"],
      "inputs": ["{workspaceRoot}/public/scripts/**/*"]
    },
    "build:translations": {
      "outputs": ["{projectRoot}/**/autoGeneratedTranslations.js"],
      "inputs": [
        "{projectRoot}/public/locales/**/*.json",
        "{workspaceRoot}/public/locales/**/*.json"
      ],
      "cache": true
    },
    "build": {
      "cache": true,
      "outputs": ["{projectRoot}/.next", "{projectRoot}/dist"]
    },
    "storybook-build": {
      "cache": true,
      "outputs": ["{projectRoot}/storybook-static"]
    },
    "test:build": {
      "cache": true,
      "outputs": ["{projectRoot}/.next"]
    },
    "test": {
      "cache": true,
      "inputs": ["{workspaceRoot}/packages/shared/**/*"]
    }
  },
  "defaultBase": "develop"
}
