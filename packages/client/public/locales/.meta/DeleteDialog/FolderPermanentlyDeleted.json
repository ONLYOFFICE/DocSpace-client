  "comment": {
    "text": "This key displays a message within a delete confirmation dialog, informing the user that a folder will be permanently removed from the trash section. It's used when deleting a folder, and the `trashSection` value should be translated separately.",
    "is_auto": true,
    "updated_at": "2025-10-01T15:20:19.777Z"
  },
