  "comment": {
    "text": "This translation key is used to display a success message when a developer token is revoked. It appears in a toast notification after clicking the \"Revoke Developer Token\" button or closing the revoke dialog. The message confirms that the token has been successfully revoked.",
    "is_auto": true,
    "updated_at": "2025-05-20T09:42:31.405Z"
  },
  "usage": [
    {
      "file_path": "/packages/client/src/pages/PortalSettings/categories/developer-tools/OAuth/sub-components/GenerateDeveloperTokenDialog.tsx",
      "line_number": 109,
      "context": "await api.oauth.revokeDeveloperToken(token, client!.clientId, secret);\n \n setRequestRunning(false);\n \n toastr.success(t(\"TokenRevokedSuccessfully\"));\n \n setToken(\"\");\n \n onClose();\n };",
      "module": "/packages/client"
    },
    {
      "file_path": "/packages/client/src/pages/PortalSettings/categories/developer-tools/OAuth/sub-components/RevokeDeveloperTokenDialog.tsx",
      "line_number": 88,
      "context": "setRequestRunning(false);\n \n setToken(\"\");\n setRevokeDeveloperTokenDialogVisible?.(false);\n \n toastr.success(t(\"OAuth:TokenRevokedSuccessfully\"));\n } catch (e) {\n toastr.error(e as TData);\n }\n };",
      "module": "/packages/client"
    }
  ],
