      "context": "scale\n hasError={errors[PORT]}\n />\n {enableAuthComponent}\n \n <Text {...commonTextProps}>{t(\"SenderDisplayName\")}</Text>\n <TextInput\n isDisabled={isLoading}\n className=\"smtp-settings_input\"\n name={SENDER_DISPLAY_NAME}\n placeholder={t(\"Common:EnterName\")}",
