{
  "name": "translations-tests",
  "version": "1.0.0",
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "test": "vitest run",
    "test:lefthook": "cross-env SKIP_BASE_LANGUAGES_TEST=true jest",
    "test:only-base-languages": "vitest run locales.test.js --testNamePattern=\"NotTranslatedOnBaseLanguages\"",
    "test:skip-base-languages": "cross-env SKIP_BASE_LANGUAGES_TEST=true vitest run locales.test.js",
    "test:locales": "vitest run locales.test.js",
    "test:images": "vitest run images.test.js",
    "test:colors": "vitest run colors.test.js",
    "test:ascii": "vitest run ascii.test.js",
    "test:dependencies": "vitest run dependencies.test.js",
    "open-report": "npx vite preview --outDir reports"
  },
  "devDependencies": {
    "@vitest/ui": "4.0.7",
    "cross-env": "^7.0.3",
    "vitest": "4.0.7"
  }
}