{
  "name": "@docspace/sdk",
  "version": "3.2.0",
  "private": true,
  "scripts": {
    "build:translations": "node ./scripts/buildTranslations.js",
    "build": "next build",
    "start": "NODE_ENV=development PORT=5099 node server.js",
    "start-prod": "NODE_ENV=production node server.js",
    "start:internal": "NODE_ENV=development API_HOST=http://127.0.0.1 node server.js",
    "start-prod:internal": "NODE_ENV=production API_HOST=http://127.0.0.1 node server.js",
    "lint": "next lint --max-warnings=0",
    "clean": "shx rm -rf .next",
    "deploy": "shx --silent mkdir -p ../../../publish/web/sdk && shx --silent mkdir -p ../../../publish/web/sdk/.next && shx --silent mkdir -p ../../../publish/web/sdk/config && shx cp -r config/* ../../../publish/web/sdk/config && shx --silent mkdir -p ../../../publish/web/sdk/node_modules && shx --silent mkdir -p ../../../publish/web/sdk/.next/static && shx cp -r .next/standalone/node_modules/* ../../../publish/web/sdk/node_modules && shx cp -r .next/static/* ../../../publish/web/sdk/.next/static    && shx cp -r .next/standalone/packages/sdk/.next/* ../../../publish/web/sdk/.next &&  shx cp -f server.prod.js ../../../publish/web/sdk/server.js && shx cp -f logger.mjs ../../../publish/web/sdk/logger.mjs && shx --silent mkdir -p ../../../publish/web/sdk/config && shx cp -r config/* ../../../publish/web/sdk/config",
    "test:build": "E2E_TEST=true next build",
    "test:start": "PORT=5112 NODE_ENV=production E2E_TEST=true node server.js",
    "test:e2e:docker:build": "docker-compose build",
    "test:e2e:docker:start": "docker-compose run --rm tests-local npx playwright test",
    "test:e2e:docker:clear": "docker rmi tests",
    "test:clean-failed-tests": "node scripts/cleanFailedTests.js"
  },
  "dependencies": {
    "date-and-time": "3.6.0",
    "classnames": "2.5.1",
    "framer-motion": "^12.0.3",
    "i18next": "20.6.1",
    "mobx": "6.13.3",
    "mobx-react": "7.6.0",
    "moment": "2.30.1",
    "moment-timezone": "0.5.46",
    "nconf": "0.12.1",
    "next": "15.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "13.5.0",
    "sass": "1.79.5",
    "styled-components": "5.3.11",
    "winston": "3.17.0",
    "winston-cloudwatch": "6.3.0",
    "winston-daily-rotate-file": "5.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "3.816.0",
    "@svgr/webpack": "8.1.0",
    "@types/node": "22.7.5",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.2",
    "prettier": "3.3.3",
    "shx": "0.3.4",
    "typescript": "5.6.3",
    "@playwright/test": "1.48.0"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start-prod": {
        "dependsOn": [
          "build"
        ]
      },
      "start:internal": {
        "dependsOn": [
          "build"
        ]
      },
      "start-prod:internal": {
        "dependsOn": [
          "build"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
