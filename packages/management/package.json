{
  "name": "@docspace/management",
  "version": "3.0.2",
  "private": true,
  "homepage": "/management",
  "scripts": {
    "build": "webpack --mode production",
    "build:translations": "node scripts/buildTranslations.js",
    "clean": "shx rm -rf dist",
    "deploy": "shx --silent mkdir -p ../../../publish/web/management && shx cp -r dist/* ../../../publish/web/management",
    "start": "webpack-cli serve",
    "start-prod": "ws --port 5015 --directory dist --spa index.html",
    "start:internal": "webpack-cli serve",
    "start-prod:internal": "ws --port 5015 --directory dist --spa index.html"
  },
  "dependencies": {
    "@babel/runtime": "catalog:",
    "@docspace/shared": "workspace:*",
    "hex-rgb": "catalog:",
    "i18next": "catalog:",
    "lodash": "catalog:",
    "mobx-react": "catalog:",
    "mobx": "catalog:",
    "react-device-detect": "catalog:",
    "react-dom": "catalog:",
    "react-i18next": "catalog:",
    "react-router-dom": "catalog:",
    "react-svg": "catalog:",
    "react": "catalog:",
    "styled-components": "catalog:",
    "windows-iana": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@babel/plugin-proposal-class-properties": "catalog:",
    "@babel/plugin-proposal-export-default-from": "catalog:",
    "@babel/plugin-transform-runtime": "catalog:",
    "@babel/preset-env": "catalog:",
    "@babel/preset-react": "catalog:",
    "@babel/preset-typescript": "catalog:",
    "@svgr/webpack": "catalog:",
    "@types/accept-language-parser": "catalog:",
    "@types/compression": "catalog:",
    "@types/cookie-parser": "catalog:",
    "@types/express": "catalog:",
    "@types/i18next-fs-backend": "catalog:",
    "@types/morgan": "catalog:",
    "@types/node": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react": "catalog:",
    "@types/styled-components": "catalog:",
    "@types/winston": "catalog:",
    "babel-loader": "catalog:",
    "clean-webpack-plugin": "catalog:",
    "copy-webpack-plugin": "catalog:",
    "css-loader": "catalog:",
    "external-remotes-plugin": "catalog:",
    "file-loader": "catalog:",
    "html-webpack-plugin": "catalog:",
    "json-loader": "catalog:",
    "local-web-server": "catalog:",
    "mocha-junit-reporter": "catalog:",
    "mocha-multi": "catalog:",
    "mocha": "catalog:",
    "mochawesome": "catalog:",
    "npm-run-all": "catalog:",
    "sass-loader": "catalog:",
    "sass": "catalog:",
    "shx": "catalog:",
    "source-map-loader": "catalog:",
    "style-loader": "catalog:",
    "terser-webpack-plugin": "catalog:",
    "typescript": "catalog:",
    "webpack-cli": "catalog:",
    "webpack-dev-server": "catalog:",
    "webpack-filter-warnings-plugin": "catalog:",
    "webpack-manifest-plugin": "catalog:",
    "webpack-merge": "catalog:",
    "webpack": "catalog:"
  },
  "title": "ONLYOFFICE",
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start:internal": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start-prod": {
        "dependsOn": [
          "build"
        ]
      },
      "start-prod:internal": {
        "dependsOn": [
          "build:translations",
          "build"
        ]
      },
      "lint": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "lint:fix": {
        "dependsOn": [
          "build:translations"
        ]
      }
    }
  }
}
