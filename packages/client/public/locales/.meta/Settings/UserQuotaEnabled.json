  "comment": {
    "text": "The \"UserQuotaEnabled\" translation key is used to display a success message when user quota has been successfully enabled. It is typically displayed as a toast notification in an application, providing confirmation to users that their quotas have been set up correctly after enabling it.",
    "is_auto": true,
    "updated_at": "2025-05-20T09:44:09.532Z"
  },
  "usage": [
    {
      "file_path": "/packages/shared/store/CurrentQuotaStore.ts",
      "line_number": 471,
      "context": "const isEnable = +quota !== -1;\n \n try {\n await setDefaultUserQuota(isEnable, +quota);\n const toastrText = isEnable\n ? t(\"UserQuotaEnabled\")\n : t(\"UserQuotaDisabled\");\n \n toastr.success(toastrText);\n } catch (e: unknown) {\n toastr.error(e as TData);",
      "module": "/packages/shared"
    }
  ],
