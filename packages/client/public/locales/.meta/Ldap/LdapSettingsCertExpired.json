  "comment": {
    "text": "This translation key is used to display a notification when an LDAP certificate has expired. It appears in the CertificateDialog.js file as part of an error message, indicating that action needs to be taken to update or renew the certificate. The translation will help inform users about the status of their LDAP connection.",
    "is_auto": true,
    "updated_at": "2025-05-20T09:42:02.457Z"
  },
