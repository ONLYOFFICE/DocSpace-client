{
  "name": "@docspace/management",
  "version": "3.0.3",
  "private": true,
  "homepage": "/management",
  "scripts": {
    "build": "webpack --mode production",
    "build:translations": "node scripts/buildTranslations.js",
    "clean": "shx rm -rf dist",
    "deploy": "shx --silent mkdir -p ../../../publish/web/management && shx cp -r dist/* ../../../publish/web/management",
    "start": "webpack-cli serve",
    "start-prod": "ws --port 5015 --directory dist --spa index.html",
    "start:internal": "webpack-cli serve",
    "start-prod:internal": "ws --port 5015 --directory dist --spa index.html"
  },
  "dependencies": {
    "@babel/runtime": "7.25.7",
    "@docspace/shared": "workspace:*",
    "hex-rgb": "5.0.0",
    "i18next": "23.7.19",
    "lodash": "4.17.21",
    "mobx-react": "7.6.0",
    "mobx": "6.13.5",
    "react-device-detect": "2.2.3",
    "react-dom": "18.3.1",
    "react-i18next": "14.0.1",
    "react-router-dom": "6.27.0",
    "react-svg": "16.1.34",
    "react": "18.3.1",
    "styled-components": "5.3.11",
    "windows-iana": "5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.8",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/plugin-proposal-export-default-from": "7.25.8",
    "@babel/plugin-transform-runtime": "7.25.7",
    "@babel/preset-env": "7.25.8",
    "@babel/preset-react": "7.25.7",
    "@babel/preset-typescript": "7.25.7",
    "@svgr/webpack": "5.5.0",
    "@types/node": "20.17.14",
    "@types/react-dom": "18.3.1",
    "@types/react": "18.3.11",
    "@types/styled-components": "5.1.34",
    "babel-loader": "8.4.1",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "9.1.0",
    "css-loader": "6.11.0",
    "eslint-webpack-plugin": "4.2.0",
    "external-remotes-plugin": "1.0.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "json-loader": "0.5.7",
    "local-web-server": "5.4.0",
    "sass-loader": "16.0.4",
    "sass": "1.79.5",
    "shx": "0.3.4",
    "source-map-loader": "3.0.2",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.10",
    "typescript": "5.6.3",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.2",
    "webpack": "5.95.0"
  },
  "title": "ONLYOFFICE",
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start:internal": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start-prod": {
        "dependsOn": [
          "build"
        ]
      },
      "start-prod:internal": {
        "dependsOn": [
          "build:translations",
          "build"
        ]
      },
      "lint": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "lint:fix": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
