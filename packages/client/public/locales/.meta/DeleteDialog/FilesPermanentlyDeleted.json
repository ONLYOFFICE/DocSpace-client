  "comment": {
    "text": "This text confirms that deleted files will be permanently removed from the trash section. It appears within a delete confirmation dialog, informing the user about the finality of the file deletion. The `{{trashSection}}` placeholder should be replaced with the localized term for the trash/recycle bin.",
    "is_auto": true,
    "updated_at": "2025-10-01T15:20:20.932Z"
  },
  "usage": [
    {
