{
  "name": "@docspace/sdk",
  "version": "3.5.0",
  "private": true,
  "scripts": {
    "build:translations": "node ./scripts/buildTranslations.js",
    "build": "next build --webpack",
    "build:deploy": "cross-env DEPLOY=true next build --webpack",
    "start": "cross-env NODE_ENV=development PORT=5099 next dev --webpack",
    "start-prod": "cross-env NODE_ENV=production node server.js",
    "lint": "eslint ./src --max-warnings=0",
    "lint:fix": "eslint ./src --fix",
    "tsc": "tsc",
    "clean": "shx rm -rf .next",
    "deploy": "shx --silent mkdir -p ../../../publish/web/sdk && shx cp -r .next/standalone/* ../../../publish/web/sdk && shx --silent mkdir -p ../../../publish/web/sdk/packages/sdk/.next/static && shx cp -r .next/static/* ../../../publish/web/sdk/packages/sdk/.next/static && shx cp -f server.prod.js ../../../publish/web/sdk/server.js",
    "test:build": "cross-env E2E_TEST=true next build --webpack",
    "test:start": "cross-env PORT=5112 NODE_ENV=production E2E_TEST=true node server.js",
    "test:e2e:docker:build": "docker-compose build",
    "test:e2e:docker:start": "docker-compose run --rm tests-local npx playwright test",
    "test:e2e:docker:clear": "docker rmi tests",
    "test:clean-failed-tests": "node scripts/cleanFailedTests.js"
  },
  "dependencies": {
    "@docspace/shared": "workspace:*",
    "classnames": "2.5.1",
    "date-and-time": "3.6.0",
    "framer-motion": "^12.0.3",
    "i18next": "23.7.19",
    "lodash": "4.17.21",
    "mobx": "6.13.5",
    "mobx-react": "7.6.0",
    "moment": "2.30.1",
    "moment-timezone": "0.5.46",
    "nconf": "0.12.1",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-i18next": "14.0.1",
    "react-virtualized": "^9.22.6",
    "sass": "1.79.5",
    "styled-components": "5.3.11",
    "ts-pattern": "5.5.0",
    "winston": "3.17.0",
    "winston-cloudwatch": "6.3.0",
    "winston-daily-rotate-file": "5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@eslint/js": "^9.34.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "7.0.0",
    "globals": "^15.0.0",
    "typescript-eslint": "^8.15.0",
    "@aws-sdk/client-cloudwatch-logs": "3.816.0",
    "@babel/core": "7.25.8",
    "@playwright/test": "1.56.1",
    "@svgr/webpack": "8.1.0",
    "@types/lodash": "4.17.10",
    "@types/node": "22.7.5",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/react-virtualized": "9.21.30",
    "@types/styled-components": "5.1.34",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint-config-next": "16.0.0",
    "prettier": "3.6.2",
    "shx": "0.3.4",
    "terser-webpack-plugin": "5.3.10",
    "typescript": "5.9.3"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "build:deploy": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start": {
        "continuous": true,
        "dependsOn": [
          "build:translations"
        ]
      },
      "start-prod": {
        "continuous": true,
        "dependsOn": [
          "build"
        ]
      },
      "lint": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "lint:fix": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "tsc": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build:deploy"
        ]
      },
      "test:build": {
        "dependsOn": [
          "build:translations"
        ]
      }
    }
  }
}
