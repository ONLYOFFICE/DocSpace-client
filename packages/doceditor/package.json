{
  "name": "@docspace/doceditor",
  "version": "3.3.0",
  "private": true,
  "scripts": {
    "build:translations": "node ./scripts/buildTranslations.js",
    "build": "next build",
    "build:deploy": "cross-env DEPLOY=true next build",
    "start": "cross-env NODE_ENV=development PORT=5013 node server.js",
    "start-prod": "cross-env NODE_ENV=production node server.js",
    "lint": "eslint ./src --max-warnings=0",
    "lint:fix": "eslint ./src --fix",
    "tsc": "tsc",
    "clean": "shx rm -rf .next",
    "deploy": "shx --silent mkdir -p ../../../publish/web/editor && shx cp -r .next/standalone/* ../../../publish/web/editor && shx --silent mkdir -p ../../../publish/web/editor/packages/doceditor/.next/static && shx cp -r .next/static/* ../../../publish/web/editor/packages/doceditor/.next/static && shx cp -f server.prod.js ../../../publish/web/editor/server.js"
  },
  "dependencies": {
    "@docspace/shared": "workspace:*",
    "@onlyoffice/document-editor-react": "file:./onlyoffice-document-editor-react-2.1.1.tgz",
    "classnames": "2.5.1",
    "crypto-js": "4.2.0",
    "date-and-time": "3.6.0",
    "framer-motion": "^12.0.3",
    "i18next": "23.7.19",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "nconf": "0.12.1",
    "next": "15.5.2",
    "react": "19.1.1",
    "react-device-detect": "2.2.3",
    "react-dom": "19.1.1",
    "react-i18next": "14.0.1",
    "sass": "1.79.5",
    "styled-components": "5.3.11",
    "ts-pattern": "5.5.0",
    "winston": "3.17.0",
    "winston-cloudwatch": "6.3.0",
    "winston-daily-rotate-file": "5.0.0",
    "@aws-sdk/client-cloudwatch-logs": "3.816.0",
    "he": "1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "5.1.0",
    "globals": "^15.0.0",
    "typescript-eslint": "^8.15.0",
    "@babel/core": "7.25.8",
    "@svgr/webpack": "8.1.0",
    "@types/crypto-js": "4.2.2",
    "@types/he": "1.2.3",
    "@types/lodash": "4.17.10",
    "@types/nconf": "0.10.7",
    "@types/node": "22.7.5",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "@types/styled-components": "5.1.34",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint-config-next": "15.5.2",
    "nconf": "0.12.1",
    "prettier": "3.6.2",
    "sass": "1.79.5",
    "shx": "0.3.4",
    "terser-webpack-plugin": "5.3.10",
    "typescript": "5.9.2"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "build:deploy": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start": {
        "continuous": true,
        "dependsOn": [
          "build:translations"
        ]
      },
      "start-prod": {
        "continuous": true,
        "dependsOn": [
          "build"
        ]
      },
      "lint": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "lint:fix": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "tsc": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build:deploy"
        ]
      }
    }
  }
}
