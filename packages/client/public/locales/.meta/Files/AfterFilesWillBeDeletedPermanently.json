  "comment": {
    "text": "This translation key is used to display a message that explains what will happen to files after they are permanently deleted. It appears in a UI element that provides users with information about file deletion options, likely on a settings page or configuration dialog. The goal of this message is to inform users about the permanent nature of the action.",
    "is_auto": true,
    "updated_at": "2025-05-20T09:40:11.990Z"
  },
  "usage": [
    {
      "file_path": "/packages/client/src/pages/Home/Section/Header/index.js",
      "line_number": 440,
      "context": "return `${t(\"Files:RoomFilesLifetime\", {\n days: lifetime.value,\n period: getLifetimePeriodTranslation(lifetime.period, t),\n })}. ${\n lifetime.deletePermanently\n ? t(\"Files:AfterFilesWillBeDeletedPermanently\")\n : t(\"Files:AfterFilesWillBeMovedToTrash\")\n }`;\n \n return null;\n };",
      "module": "/packages/client"
    }
