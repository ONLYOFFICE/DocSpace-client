{
  "extends": "../shared/tsconfig.json",
  "compilerOptions": {
    "rootDir": "./",
    "baseUrl": "./",
    "jsx": "react-jsx",
    "paths": {
      "PUBLIC_DIR/*": ["../../public/*"],
      "SRC_DIR/*": ["./src/*"],
      "PACKAGE_FILE": ["./package.json"]
    },
    "target": "esnext"
  },
  "include": ["**/*.ts", "**/*.tsx", "types/**/*.d.ts"]
}
