{
  "name": "@docspace/management",
  "version": "3.2.0",
  "private": true,
  "scripts": {
    "build:translations": "node scripts/buildTranslations.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=development PORT=5015 next dev",
    "start-prod": "cross-env NODE_ENV=production node server.js",
    "start:internal": "cross-env NODE_ENV=development API_HOST=http://127.0.0.1 next dev",
    "start-prod:internal": "cross-env NODE_ENV=production API_HOST=http://127.0.0.1 node server.js",
    "lint": "next lint --max-warnings=0",
    "clean": "shx rm -rf .next",
    "deploy": "shx --silent mkdir -p ../../../publish/web/management && shx cp -r .next/standalone/* ../../../publish/web/management && shx --silent mkdir -p ../../../publish/web/management/packages/management/.next/static && shx cp -r .next/static/* ../../../publish/web/management/packages/management/.next/static && shx cp -f server.prod.js ../../../publish/web/management/server.js"
  },
  "dependencies": {
    "@babel/runtime": "7.25.7",
    "@docspace/shared": "workspace:*",
    "classnames": "2.5.1",
    "hex-rgb": "5.0.0",
    "i18next": "23.7.19",
    "lodash": "4.17.21",
    "mobx-react": "7.6.0",
    "mobx": "6.13.5",
    "react-device-detect": "2.2.3",
    "react-i18next": "14.0.1",
    "react-svg": "16.1.34",
    "windows-iana": "5.1.0",
    "date-and-time": "3.6.0",
    "nconf": "0.12.1",
    "next": "15.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sass": "1.79.5",
    "socket.io-client": "^4.8.0",
    "styled-components": "5.3.11",
    "winston": "3.17.0",
    "winston-cloudwatch": "6.3.0",
    "winston-daily-rotate-file": "5.0.0",
    "@aws-sdk/client-cloudwatch-logs": "3.816.0"
  },
  "devDependencies": {
    "@types/lodash": "4.17.10",
    "@babel/core": "7.25.8",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/plugin-proposal-export-default-from": "7.25.8",
    "@babel/plugin-transform-runtime": "7.25.7",
    "@babel/preset-env": "7.25.8",
    "@babel/preset-react": "7.25.7",
    "@babel/preset-typescript": "7.25.7",
    "@types/styled-components": "5.1.34",
    "cross-env": "^7.0.3",
    "css-loader": "6.11.0",
    "local-web-server": "5.4.0",
    "sass-loader": "16.0.4",
    "sass": "1.79.5",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.10",
    "webpack": "5.95.0",
    "@svgr/webpack": "8.1.0",
    "@types/nconf": "^0",
    "@types/node": "22.7.5",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.2",
    "prettier": "3.3.3",
    "shx": "0.3.4",
    "typescript": "5.6.3"
  },
  "title": "ONLYOFFICE",
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start-prod": {
        "dependsOn": [
          "build"
        ]
      },
      "start:internal": {
        "dependsOn": [
          "build"
        ]
      },
      "start-prod:internal": {
        "dependsOn": [
          "build"
        ]
      },
      "lint": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "lint:fix": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}

