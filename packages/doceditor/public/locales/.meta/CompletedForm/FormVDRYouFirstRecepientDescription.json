  "comment": {
    "text": "This text appears as a header description within the completed VDR form UI. It informs the first recipient that they are assigned to fill out the form, confirms a notification has been sent, and provides instructions to start filling it out or save the link for later. This helps guide the user through the form completion process.",
    "is_auto": true,
    "updated_at": "2025-10-13T14:28:08.669Z"
  },
