{
  "compilerOptions": {
    "noEmit": false,
    "module": "ESNext",
    "target": "ES2020",
    "lib": ["ES2020", "WebWorker"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "sw/template.ts",
    "sw/config.ts",
    "sw/types.ts",
    "sw/core/**/*.ts",
    "sw/strategies/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "**/*.spec.ts",
    "sw/client/**/*.ts",
    "sw/worker.ts",
    "sw/helper.ts"
  ]
}
