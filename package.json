{
  "name": "docspace",
  "version": "3.0.2",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "before-build": "node ./common/scripts/before-build.js",
    "build": "pnpm run before-build && pnpm --filter @docspace/login --filter @docspace/client --filter @docspace/doceditor --filter @docspace/management run build",
    "bump": "pnpm version --workspace",
    "clean": "pnpm -r run clean",
    "deploy": "shx rm -rf ../publish/web && pnpm --filter @docspace/login --filter @docspace/client --filter @docspace/doceditor --filter @docspace/management run deploy && shx cp -r public ../publish/web/ && node common/scripts/minify-common-locales.js",
    "start": "pnpm run before-build && pnpm --filter @docspace/client --filter @docspace/login --filter @docspace/doceditor --filter @docspace/management run start",
    "start-prod": "pnpm --filter @docspace/client --filter @docspace/login --filter @docspace/doceditor --filter @docspace/management run start-prod",
    "start:internal": "pnpm run before-build && pnpm --filter @docspace/login --filter @docspace/client --filter @docspace/doceditor --filter @docspace/management run start:internal",
    "start-prod:internal": "pnpm --filter @docspace/login --filter @docspace/client --filter @docspace/doceditor --filter @docspace/management run start-prod:internal",
    "storybook": "pnpm --filter @docspace/shared story",
    "storybook-build": "pnpm --filter @docspace/shared run storybook-build",
    "storybook-serve": "pnpm --filter @docspace/shared run storybook-serve",
    "test": "pnpm --filter @docspace/shared test",
    "wipe": "shx rm -rf node_modules pnpm-lock.yaml packages/**/node_modules",
    "licenses-audit": "pnpm licenses audit --output-csv=licenses.csv --config=licenses.config.js --summary",
    "check-circular": "pnpm madge --circular ./packages",
    "check-circular:graph": "pnpm madge --circular --image graph.svg ./packages",
    "lint": "pnpm --filter @docspace/login --filter @docspace/client --filter @docspace/doceditor --filter @docspace/management run lint",
    "lint:fix": "pnpm --filter @docspace/login --filter @docspace/client --filter @docspace/doceditor --filter @docspace/management run lint:fix",
    "test:build": "pnpm --filter @docspace/login test:build",
    "test:e2e:docker:build": "pnpm --filter @docspace/login test:e2e:docker:build",
    "test:e2e:docker:start": "pnpm --filter @docspace/login test:e2e:docker:start",
    "test:e2e:docker:clear": "pnpm --filter @docspace/login test:e2e:docker:clear"
  },
  "devDependencies": {
    "@types/node": "20.17.14",
    "@types/react-text-mask": "5.4.14",
    "he": "1.2.0",
    "madge": "6.1.0",
    "nx": "20.3.1",
    "react": "18.3.1",
    "shx": "0.3.4",
    "terser": "5.34.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "packageManager": "pnpm@9.15.4",
  "resolutions": {
    "@types/react": "18.3.1"
  }
}
