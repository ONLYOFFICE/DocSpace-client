{
  "name": "docspace",
  "version": "3.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "node ./common/scripts/before-build.js && pnpm --filter @docspace/login --filter @docspace/client run build",
    "bump": "pnpm version --workspace",
    "temp": "node ./common/scripts/before-build.js && pnpm --filter @docspace/client --filter @docspace/login --filter @docspace/doceditor --filter @docspace/management",
    "clean": "pnpm -r run clean",
    "deploy": "shx rm -rf ../publish/web && pnpm --filter {@docspace/client,@docspace/login,@docspace/doceditor,@docspace/management} run deploy && shx cp -r public ../publish/web/ && node common/scripts/minify-common-locales.js",
    "start": "node ./common/scripts/before-build.js && pnpm --filter @docspace/client --filter @docspace/login run start",
    "start-prod": "pnpm --filter @docspace/client --filter @docspace/login run start-prod",
    "start:internal": "node ./common/scripts/before-build.js && pnpm --filter {@docspace/client,@docspace/login,@docspace/doceditor,@docspace/management} run start:internal",
    "start-prod:internal": "pnpm --filter {@docspace/client,@docspace/login,@docspace/doceditor,@docspace/management} run start-prod:internal",
    "storybook": "pnpm --filter @docspace/shared story",
    "storybook-build": "pnpm --filter @docspace/shared run storybook-build",
    "storybook-serve": "pnpm --filter @docspace/shared run storybook-serve",
    "test": "pnpm --filter @docspace/shared test",
    "wipe": "shx rm -rf node_modules pnpm-lock.yaml packages/**/node_modules",
    "licenses-audit": "pnpm licenses audit --output-csv=licenses.csv --config=licenses.config.js --summary",
    "check-circular": "pnpm madge --circular ./packages",
    "check-circular:graph": "pnpm madge --circular --image graph.svg ./packages",
    "lint": "pnpm --filter {@docspace/login,@docspace/doceditor,@docspace/client} run lint",
    "lint:fix": "pnpm --filter {@docspace/login,@docspace/doceditor,@docspace/client} run lint:fix",
    "test:build": "pnpm --filter @docspace/login test:build",
    "test:e2e:docker:build": "pnpm --filter @docspace/login test:e2e:docker:build",
    "test:e2e:docker:start": "pnpm --filter @docspace/login test:e2e:docker:start",
    "test:e2e:docker:clear": "pnpm --filter @docspace/login test:e2e:docker:clear"
  },
  "old-scripts": {
    "build:test": "pnpm --filter {@docspace/client,@docspace/login,@docspace/editor} run build:test",
    "build:test.translation": "pnpm --filter {@docspace/client,@docspace/login,@docspace/editor} run build:test.translation",
    "e2e.test": "pnpm --filter {@docspace/client,@docspace/login} run test:sequential",
    "e2e.test:sequential": "pnpm --filter @docspace/client test:sequential && pnpm --filter @docspace/login test:sequential",
    "e2e.test:model": "pnpm --filter @docspace/client test:model && pnpm --filter @docspace/login test:model",
    "e2e.test:translation": "pnpm --filter {@docspace/client,@docspace/login} run test:translation:model"
  },
  "devDependencies": {
    "@types/react-text-mask": "catalog:",
    "he": "catalog:",
    "madge": "catalog:",
    "shx": "catalog:",
    "terser": "catalog:",
    "react": "catalog:"
  },
  "packageManager": "pnpm@9.15.3"
}
