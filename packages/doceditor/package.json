{
  "name": "@docspace/doceditor",
  "version": "3.0.4",
  "private": true,
  "scripts": {
    "build:translations": "node ./scripts/buildTranslations.js",
    "build": "next build",
    "start": "NODE_ENV=development node server.js",
    "start-prod": "NODE_ENV=production node server.js",
    "start:internal": "NODE_ENV=development API_HOST=http://127.0.0.1 node server.js",
    "start-prod:internal": "NODE_ENV=production API_HOST=http://127.0.0.1 node server.js",
    "lint": "next lint --max-warnings=0",
    "clean": "shx rm -rf .next",
    "deploy": "node ./scripts/deploy.js"
  },
  "dependencies": {
    "@docspace/shared": "workspace:*",
    "@onlyoffice/document-editor-react": "1.6.0",
    "@serdnam/pino-cloudwatch-transport": "1.0.4",
    "crypto-js": "4.2.0",
    "framer-motion": "12.0.3",
    "i18next": "23.7.19",
    "moment": "2.30.1",
    "nconf": "0.12.1",
    "next": "14.2.23",
    "pino-pretty": "11.3.0",
    "pino-roll": "2.2.0",
    "pino": "9.5.0",
    "react-device-detect": "2.2.3",
    "react-dom": "18.3.1",
    "react-i18next": "14.0.1",
    "react": "18.3.1",
    "styled-components": "5.3.11",
    "ts-pattern": "5.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.8",
    "@svgr/webpack": "5.5.0",
    "@types/crypto-js": "4.2.2",
    "@types/nconf": "0.10.7",
    "@types/node": "20.17.14",
    "@types/he": "1.2.3",
    "@types/react-dom": "18.3.1",
    "@types/react": "18.3.11",
    "@types/styled-components": "5.1.34",
    "eslint-config-next": "14.2.15",
    "eslint": "8.57.1",
    "nconf": "0.12.1",
    "pino-pretty": "11.3.0",
    "pino-roll": "2.2.0",
    "pino": "9.5.0",
    "prettier": "3.3.3",
    "sass": "1.79.5",
    "shx": "0.3.4",
    "terser-webpack-plugin": "5.3.10",
    "typescript": "5.6.3"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:translations"
        ]
      },
      "start": {
        "dependsOn": [
          "build"
        ]
      },
      "start-prod": {
        "dependsOn": [
          "build"
        ]
      },
      "start:internal": {
        "dependsOn": [
          "build"
        ]
      },
      "start-prod:internal": {
        "dependsOn": [
          "build"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
